<%= form_for(@project) do |f| %>
  <%= render 'layouts/error_messages' , object: f.object %>
  
   <%= f.label :title %>
   <%= f.text_field :title %>

   <%= f.label :description %>
   <%= f.text_field :description %>

   <%= f.label :image %>
   <%= f.file_field :image %>

   <%= f.label :file %>
   <%= f.file_field :file %>

  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>